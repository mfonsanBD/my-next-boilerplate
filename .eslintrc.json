{
  "extends": [
    "eslint:recommended",
    "next",
    "prettier",
    "next/core-web-vitals",
    "plugin:storybook/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "@next/next/no-img-element": "off",
    "no-undef": "off",
    "typescript-eslint/no-explicit-any": "off"
  }
}
